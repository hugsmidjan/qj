{
  "name": "qj",
  "version": "2.5.0",
  "description": "Quick JavaScripts – Collection of useful miniature JavaScript helper functions.",
  "scripts": {
    "postinstall": "node build/postinstall.js",
    "_test": "ospec '_tests/**/*.js'",
    "_test:dev": "sleep 1  &&  yarn run _test  &&  onchange '_tests/**' -f add,change -- ospec {{changed}}",
    "_build": "yarn run clean  &&  node build/prebuild.js  &&  rollup -c build/rollup.config.js",
    "clean": "rm -rf dist _tests",
    "test": "yarn run _build  &&  yarn run _test",
    "build": "npm run _build  &&  npm run _test",
    "dev": "yarn run _test:dev  &  yarn run _build -w"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/hugsmidjan/qj.git"
  },
  "author": "Hugsmiðjan ehf. (www.hugsmidjan.is)",
  "contributors": [
    "Már Örlygsson <mar@hugsmidjan.is>"
  ],
  "engines": {
    "node": ">=6",
    "yarn": ">=1.11",
    "npm": ">=6.4"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jsdom": "^12.2.4",
    "glob": "^7.1.4",
    "jsdom": "^15.1.1",
    "onchange": "^6.0.0",
    "ospec": "^4.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-typescript2": "^0.22.0",
    "typescript": "^3.5.3"
  }
}
