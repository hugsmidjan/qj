{
  "name": "qj",
  "version": "1.17.0",
  "description": "Quick JavaScripts – Collection of useful miniature JavaScript helper functions.",
  "module": "index.js",
  "main": "cjs/index.js",
  "scripts": {
    "_test": "ospec 'cjs/_tests/**/*.js'",
    "_test:dev": "onchange 'cjs/_tests/**' -f add,change -- ospec {{changed}}",
    "_build": "rollup -c build/rollup.config.js",
    "test": "yarn run _build && yarn run _test",
    "build": "rm -rf cjs && npm run _build && npm run _test",
    "dev": "yarn run _test:dev & yarn run _build -w"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/hugsmidjan/qj.git"
  },
  "author": "Hugsmiðjan ehf. (www.hugsmidjan.is)",
  "contributors": [
    "Már Örlygsson <mar@hugsmidjan.is>"
  ],
  "license": "ISC",
  "devDependencies": {
    "onchange": "^4.0",
    "ospec": "^3.0",
    "rollup": "^0.63"
  }
}
